{
  "schemaVersion": 1,
  "id": "whymap",
  "version": "${version}",

  "name": "WhyMap",
  "description": "WhyMap is a fairplay worldmap mod created by wefhy",
  "authors": [
    "wefhy"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "Apache-2.0",
  "icon": "assets/whymap/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.wefhy.whymap.WhyMapMod"
      }
    ]
  },
  "mixins": [
    "whymap.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric-language-kotlin": ">=1.8.2+kotlin.1.7.10",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
